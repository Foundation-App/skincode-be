{
  "name": "skincode-be",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "jest --verbose --runInBand --testLocationInResults --setupFiles dotenv/config",
    "test:watch": "npm run test -- --watch",
    "start": "node -r dotenv/config server.js",
    "start:watch": "nodemon -r dotenv/config server.js",
    "setup-db": "npm run recreate-tables && npm run load-seed-data",
    "setup-heroku": "heroku addons:create heroku-postgresql:hobby-dev && heroku config:set PGSSLMODE=require && heroku config:get DATABASE_URL",
    "heroku-postbuild": "npm run build",
    "create-tables": "node data/create-tables.js",
    "drop-tables": "node data/drop-tables.js",
    "load-seed-data": "node data/load-seed-data.js",
    "recreate-tables": "npm run drop-tables && npm run create-tables",
    "setup-db:prod": "heroku run npm run setup-db"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@types/jest": "^26.0.22",
    "@types/pg": "^7.14.11",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "clarifai": "^2.9.1",
    "color-difference": "^0.3.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node": "^16.0.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "superagent": "^6.1.0"
  },
  "engines": {
    "npm": "5.6.0",
    "node": "8.10.0"
  }
}
